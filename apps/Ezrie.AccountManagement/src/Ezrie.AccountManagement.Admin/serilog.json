{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"Microsoft.EntityFrameworkCore": "Warning"
			}
		},
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"outputTemplate": "[{Level:u3}] <{SourceContext:l}> {Message}{NewLine}{Exception}"
				}
			},
			{
				"Name": "Seq",
				"Args": { "serverUrl": "http://localhost:5341" }
			}
		]
	}
}
