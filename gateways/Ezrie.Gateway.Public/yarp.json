{
	"ReverseProxy": {
		"Routes": {
			"Ezrie Administration Serivce": {
				"ClusterId": "ezrie-administration-httpapi-host",
				"Match": {
					"Path": "/api/administration/{**everything}"
				}
			},
			"Ezrie Tenant Serivce": {
				"ClusterId": "ezrie-tenant-httpapi-host",
				"Match": {
					"Path": "/api/tenant/{**everything}"
				}
			},
			"Ezrie Account Management": {
				"ClusterId": "ezrie-account-httpapi-host",
				"Match": {
					"Path": "/api/account/{**everything}"
				}
			}
		},
		"Clusters": {
			"ezrie-administration-httpapi-host": {
				"Destinations": {
					"destination1": {
						"Address": "https://localhost:10001"
					}
				}
			},
			"ezrie-tenant-httpapi-host": {
				"Destinations": {
					"destination1": {
						"Address": "https://localhost:11001"
					}
				}
			},
			"ezrie-account-httpapi-host": {
				"Destinations": {
					"destination1": {
						"Address": "https://localhost:5001"
					}
				}
			}
		}
	}
}
