{
	"ReverseProxy": {
		"Clusters": {
			"ezrie-crm-app": {
				"Destinations": {
					"destination1": {
						"Address": "https://localhost:10000"
					}
				}
			},
			"ezrie-administration-service-api": {
				"Destinations": {
					"destination1": {
						"Address": "https://localhost:10200"
					}
				}
			},
			"ezrie-identity-service-api": {
				"Destinations": {
					"destination1": {
						"Address": "https://localhost:10300"
					}
				}
			},
			"ezrie-tenant-service-api": {
				"Destinations": {
					"destination1": {
						"Address": "https://localhost:10400"
					}
				}
			}
		},
		"Routes": {
			"ABP": {
				"ClusterId": "ezrie-administration-service-api",
				"Match": {
					"Path": "/api/{**everything}"
				}
			},
			"Ezrie Administration Serivce": {
				"ClusterId": "ezrie-administration-service-api",
				"Match": {
					"Path": "/api/administration/{**everything}"
				},
				"Transforms": [
					{
						"PathPattern": "/api/{**remainder}"
					}
				]
			},
			"Ezrie Identity Serivce": {
				"ClusterId": "ezrie-identity-service-api",
				"Match": {
					"Path": "/api/identity/{**everything}"
				},
				"Transforms": [
					{
						"PathPattern": "/api/{**remainder}"
					}
				]
			},
			"tenant": {
				"ClusterId": "ezrie-tenant-service-api",
				"Match": {
					"Path": "/api/tenant/{**everything}"
				}
			},
			"multi-tenancy": {
				"ClusterId": "ezrie-tenant-service-api",
				"Match": {
					"Path": "/api/multi-tenancy/{**everything}"
				}
			},
			"EzrieCRM": {
				"ClusterId": "ezrie-crm-app",
				"Match": {
					"Path": "{**catch-all}"
				}
			}
		}
	}
}
